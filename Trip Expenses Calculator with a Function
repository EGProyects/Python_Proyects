def calculate_expenses(plane_ticket_price, car_rental_rate, hotel_rate, trip_time):
  car_rental_total = car_rental_rate * trip_time
  hotel_total = (hotel_rate * trip_time) - 10
  trip_total = car_rental_total + plane_ticket_price + hotel_total
  print('Your plane ticket price is: $' + str(plane_ticket_price) + ' USD, your car rental price is: $' + str(car_rental_total) + ' USD, and your hotel price is: $' + str(hotel_total) + ' USD. It adds up to a total of: $' + str(trip_total) +' USD')
calculate_expenses(200, 100, 100, 5)
